{
    "buildCommand": "npm run build",
    "outputDirectory": "dist/public",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        }
    ],
    "functions": {
        "api/**/*.ts": {
            "runtime": "@vercel/node@3.0.0"
        }
    }
}